function divideNumbers(){
    try{
        let number = 10;
        let zero = 0;
    

        if(zero===0){
            throw new Error("0으로는 나눌 수 없습니다.")
        }
    let result = number / zero;
        console.log("결과: "+result);
    }catch(e){
        console.log("문제가 생겼어요."+e.message);
    }finally{
        console.log("프로그램이 끝났어요.");
    }
}
